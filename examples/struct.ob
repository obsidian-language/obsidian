fn createPerson(string name, i32 age) void {
    struct NewPerson {
        string name
        i32 age
    }

    NewPerson.name = name;
    NewPerson.age = age;

    println(NewPerson.name);
    println(NewPerson.age);
}

fn main() i32 {
    createPerson("azlo", 19);
    createPerson("john", 10);
    return 0;
}