// Functions can have all return types
fn a_i8() i8 {}
fn a_i16() i16 {}
fn a_i32() i32 {}
fn a_i64() i64 {}
fn a_u8() u8 {}
fn a_u16() u16 {}
fn a_u32() u32 {}
fn a_u64() u64 {}
fn b_f32() f32 {}
fn b_f64() f64 {}
fn c() string {}
fn d() char {}
fn e() bool {}
fn f() void {}

// Entry point
fn main() i32 {
    // Functions can be called by <function_name> or can be decl as variables
    i32 x = a_i32();
    f();
    return 0;
}